               var list=res.data.data;\r\n                   this.list=list;\r\n               }\r\n           })\r\n         },\r\n         numUp(item){\r\n            item.gcount++;\r\n        },\r\n        numDown(item){\r\n            item.gcount--;\r\n            if(item.gcount<1){\r\n                var gid=item.gid;\r\n                this.$messagebox.confirm(\"是否删除指定商品\").then(res=>{\r\n                var url=\"del\";\r\n                var obj={gid:gid};\r\n                this.axios.get(url,{params:obj}).then(res=>{\r\n                   if(res.data.code>0){\r\n                       this.$toast(\"删除成功\");\r\n                        this.$router.push({\r\n                        path:'Home'\r\n                })\r\n                   }\r\n               })\r\n            }).catch(err=>{\r\n                 item.gcount++;\r\n            })\r\n         }\r\n            },\r\n    },\r\n    created() {\r\n        this.loadCart();\r\n    },\r\n}\r\n</script>\r\n<style scoped>\r\n.Buy-Cart{\r\n    font-size: 16px;\r\n}\r\n.Buy-Cart>div{\r\n    width: 100%;\r\n    height:70px;\r\n    padding-top: 15px;\r\n    border-bottom: 2px solid #e9e7e8;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-flow:column wrap;\r\n}\r\n.Buy-Cart>div>div{\r\n    width: 90%;\r\n    /* margin: 0 auto;  */\r\n}\r\n.Buycart>div>div{\r\n    display: inline-block;\r\n    vertical-align: top;\r\n}\r\n.Buycart img{\r\n    width:60px;\r\n    height: 60px;\r\n}\r\n.Buycart_title{\r\n    margin-left: 15px;\r\n    width: 100px;\r\n}\r\n.Buycart_title p{\r\n    height: 25px;\r\n    margin: 0;\r\n    white-space: nowrap;\r\n}\r\n.title_price{\r\n    color: #ff7100;\r\n}\r\n.Buycart_num{\r\n    font-size:25px;\r\n    float: right;\r\n\r\n}\r\n.Buycart_num span{\r\n    padding-left:10px;\r\n    padding-right: 10px; \r\n}\r\n</style>"]}]}